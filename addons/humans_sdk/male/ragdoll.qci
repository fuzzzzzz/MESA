$surfaceprop "flesh"

$cdmaterials models\humans\male
$collisionjoints ragdoll {

	$mass 90.0
	$inertia 10.00
	$damping 0.01
	$rotdamping 1.50
	$rootbone "valvebiped.bip01_pelvis"
